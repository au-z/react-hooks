{
  "name": "@auzmartist/react-hooks",
  "version": "0.1.1",
  "description": "Helpful hooks to satisfy common reactive patterns",
  "main": "index.js",
  "scripts": {
    "build": "webpack --env.prod --config ./build/webpack.config.js --progress",
    "dev": "webpack --env.dev -w --config ./build/webpack.config.js --progress",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "react",
    "hooks",
    "computed"
  ],
  "author": "Austin Martin (auz.martin@gmail.com)",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/jest": "^26.0.14",
    "babel-jest": "^26.5.2",
    "core-js": "^3.6.5",
    "jest": "^26.4.2",
    "react": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "peerDependencies": {
    "react": "^16.13.1"
  },
  "jest": {
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "verbose": true
  },
  "dependencies": {
    "@types/react": "^16.9.51",
    "babel-loader": "^8.1.0"
  }
}
